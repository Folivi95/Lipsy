@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h2>All Lipsticks are shown here.</h2>
</div>

<h2>@Model.CurrentCategory</h2>

<div class="row">
    @{
        foreach (Lipstick lipstick in Model.Lipsticks)
        {
            @await Html.PartialAsync("LipsticksSummary", lipstick)
        }
    }
</div>
        

